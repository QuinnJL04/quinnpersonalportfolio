{
    "builds": [
      {
        "src": "react-portfolio/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "react-portfolio/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "react-portfolio/$1"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/react-portfolio/index.html" }
    ]
  }
  